{
    "version": 2,
    "builds": [
        {
            "src": "index.cjs",
            "use": "@now/node"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "index.cjs"
        }
    ],
    "crons": [
    {
      "path": "/delete/complaints",
      "schedule": "0 2 * * *"
    }
  ]
}
